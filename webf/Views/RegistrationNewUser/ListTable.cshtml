
@model IEnumerable<webf.Models.EntityModel.UserProfile>
           
<table> 
        <tr>
            <th>
                LastName
            </th>
            <th>
                FirstName
            </th>
            <th>
                ParentName
            </th>
            <th>
                MilitaryDegree
            </th>
            <th>
                DateOfBirth
            </th>
            <th>
                aspnet_Users
            </th>
            <th>
                KeyTable
            </th>
            <th>
                UserProfile2
            </th>
            <th>
                Posts
            </th>
            <th>
                WorkArea
            </th>
            <th>
                Region
            </th>
            <th>
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ParentName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MilitaryDegree.DegreeName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateOfBirth)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.aspnet_Users.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.KeyTable.HostIdentity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserProfile2.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Posts.PostName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WorkArea.WA_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Region.RegionName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.UserProfileID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.UserProfileID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.UserProfileID })
                </td>
            </tr>
        }
    </table>