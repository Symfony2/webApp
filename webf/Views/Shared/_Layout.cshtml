<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/jquery-ui-1.8.20.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/SpecialOwn.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/jquery.Jcrop.css")" rel="stylesheet" type="text/css" />
    

    @Content.Script("jquery-1.8.2.min.js", Url)
    @Content.Script("jquery-ui-1.8.20.custom.min.js", Url)
    @Content.Script("CodeGallery.js", Url)
    @Content.Script("PaginationExp.js", Url)
    @Content.Script("jquery.Jcrop.min.js",Url)
    @RenderSection("scripts", false)
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>
                    My MVC Application</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
            
        </div>
    </div>
    
</body>
</html>
